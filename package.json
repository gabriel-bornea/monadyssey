{
  "name": "monadyssey",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "version": "1.7.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabriel-bornea/monadyssey"
  },
  "keywords": [
    "functional programming",
    "typescript"
  ],
  "author": "Gabriel Bornea",
  "contributors": [
    {
      "name": "Tiberiu Tofan"
    }
  ],
  "scripts": {
    "lint": "eslint \"packages/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"packages/**/*.{ts,tsx}\" --fix",
    "prettier": "prettier \"packages/**/*.{ts,tsx}\" --check",
    "prettier:fix": "prettier \"packages/**/*.{ts,tsx}\" --write",
    "format": "npm run lint:fix && npm run prettier:fix",
    "test": "jest --workspaces --verbose --coverage",
    "build": "npm run format && tsc && npm run build --workspaces"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "8.18.0",
    "@typescript-eslint/parser": "8.18.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.3.0"
  }
}